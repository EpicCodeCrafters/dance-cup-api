{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "GrpcEndpoint": {
        "Url": "http://*:80",
        "Protocols": "Http2"
      },
      "MetricsEndpoint": {
        "Url": "http://*:81",
        "Protocols": "Http1"
      }
    }
  }
}