{
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "GrpcEndpoint": {
        "Url": "http://*:80",
        "Protocols": "Http2"
      },
      "MetricsEndpoint": {
        "Url": "http://*:81",
        "Protocols": "Http1"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    }
  }
}